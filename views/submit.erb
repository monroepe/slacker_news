<div id = "articles">
<h1>Add Articles</h1>
  <form class = "comment_form" action= '/submit' method = 'post'>
   Title: <input type = "text" name = "title" value = "<%= params["title"]%>">
   URL: <input type = "text" name = "url" value = "<%= params["url"]%>">
   Description: <input type = "text" name = "description" value = "<%= params["description"]%>">
    <input type = "submit" value = "Add Article">
  </form>
</div>
